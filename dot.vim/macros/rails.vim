Rnavcommand config config -suffix= -default=routes.rb
Rnavcommand serializer app/serializers -suffix=_serializer.rb -default=model()

Rnavcommand jmodel app/assets/javascripts/models -suffix=.js.coffee
Rnavcommand jcontroller app/assets/javascripts/controllers -suffix=_controller.js.coffee
Rnavcommand jview app/assets/javascripts/views -suffix=_view.js.coffee
Rnavcommand jtemplate app/assets/javascripts/templates -suffix=.js.hbs.hamlbars
